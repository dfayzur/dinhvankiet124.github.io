      ]
     },
     "execution_count": 2,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "#Select all the cases where Baltimore's district name is \"North\"\n",
    "district[grep(\"NORTH\", as.character(district))]"
   ]
  }
