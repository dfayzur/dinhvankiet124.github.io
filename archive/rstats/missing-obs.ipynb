   "source": [
    "# Removing Missing Observations"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "# Create some data with missing observations\n",
    "ages <- c(3, 4, 9, NA, 93, 2, NA, NA, 2, 0, 2, 9)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "# Create a new variable that is a subset that doesn't include the missing observations\n",
    "ages.no.na <- subset(ages, !is.na(ages))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {
